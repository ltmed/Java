
public class Assignmnet6 {
	
	public static void main(String []args) {
		
		String s1="Computer Science";
		int x=307;
		String s2=s1+" "+x;
		String s3=s2.substring(7,10);
		String s4="is fun";
		String s5=s2+s4;
		System.out.println(s1+"\n"+s2+"\n"+s3+"\n"+s4+"\n"+s5);
		
		x=3;
		int y=5;
		String s6=x+y+"total";
		String s7="total"+x+y;
		String s8=x+" "+y+" "+"total";
		System.out.println(s6+"\n"+s7+"\n"+s8);
	}

}
